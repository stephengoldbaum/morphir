{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "CreateElement": {
      "type": "object",
      "properties": {
        "requestId": { "type": "string" },
        "domain": { "type": "array", "items": { "type": "string" } },
        "element": { "$ref": "DataThread.schema.json#/definitions/Element" }
      },
      "required": ["requestId", "domain", "element"]
    },
    "CreateDataset": {
      "type": "object",
      "properties": {
        "requestId": { "type": "string" },
        "domain": { "type": "array", "items": { "type": "string" } },
        "dataset": { "$ref": "DataThread.schema.json#/definitions/Dataset" }
      },
      "required": ["requestId", "domain", "dataset"]
    }
  }
}
